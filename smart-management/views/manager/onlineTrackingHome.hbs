<div class="movap-titulo">
    <h4 align="left">ACOMPANHAMENTO ONLINE</h4>
    <hr>
</div>
<script>atualizar();</script>

<div class="col">
    <div class="row">
        <div class="col-5">
            <br>
            <div class="row">
              <button class="listagem" onclick="listView()"><i class="fa fa-bars"></i></button>
              <button class="gradeamento" onclick="gridView()"><i class="fa fa-th-large"></i></button>
            </div>
            <div class="row">
                <div class="input-group">
                    <input id="myInput" class="form-control" type="text" onkeyup="search()" placeholder="Pesquisar"
