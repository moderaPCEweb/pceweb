                          <div class="nomestatusdesligado">Teodora Sampaio</div>
                        </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="column emuso">
                        <div>
                          <img class="imgstatus" src="/images/emuso.png">
                          <span class="txtemuso">EM USO</span>
                          <div class="nomestatusemuso">Teodora Sampaio</div>
                        </div>
                    </div>
                  </div>
                    <tbody class="table-body">
                        <!-- {{#each stations}}
                    <tr>
                    <th scope="row" class="font-book text-left">{{nameEmployed}}</th>
                    <th id="Atualizar" scope="row" class="font-book text-center {{dataesp}}">{{dataesp}}</th>
                    </tr>
                    {{/each}} -->
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
